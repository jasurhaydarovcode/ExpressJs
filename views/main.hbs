<main>
    <h1>Users List</h1>
    {{#if users}}
    {{#each users}}
        <div class="card">
            <img src="https://www.w3schools.com/howto/img_avatar.png" alt="Avatar" style="width: 100%;">
            <div class="container">
                <h4><b>Name: {{username}}</b></h4>
                <p>Age: {{ age }}</p>
            </div>
        </div>
    {{/each}}
    {{else}}
        <p>All User List...</p>
    {{/if}}
</main>